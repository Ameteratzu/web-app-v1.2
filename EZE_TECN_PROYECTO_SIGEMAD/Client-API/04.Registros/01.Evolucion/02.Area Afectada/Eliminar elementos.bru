meta {
  name: Eliminar elementos
  type: http
  seq: 3
}

post {
  url: {{Server}}/api/v1/evoluciones/areas-afectadas
  body: json
  auth: inherit
}

body:json {
  {
    "IdSuceso": {{IdSuceso}},
    "IdRegistroActualizacion": 1,
    "AreasAfectadas": []
  }
}

vars:post-response {
  IdEvolucion: res.body.idEvolucion
}

assert {
  res.status: eq 200
}
