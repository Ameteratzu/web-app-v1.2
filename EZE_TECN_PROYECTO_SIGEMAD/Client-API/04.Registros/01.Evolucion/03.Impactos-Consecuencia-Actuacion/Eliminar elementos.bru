meta {
  name: Eliminar elementos
  type: http
  seq: 3
}

post {
  url: {{Server}}/api/v1/evoluciones/impactos
  body: json
  auth: inherit
}

body:json {
  {
    "IdEvolucion": {{IdEvolucion}},
    "IdSuceso": 1,
    "Impactos": []
  }
}

vars:post-response {
  IdEvolucion: res.body.idEvolucion
}

assert {
  res.status: eq 200
}
