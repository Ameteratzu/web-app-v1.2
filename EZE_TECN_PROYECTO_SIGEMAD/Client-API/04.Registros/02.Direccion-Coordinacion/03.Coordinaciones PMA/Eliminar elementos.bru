meta {
  name: Eliminar elementos
  type: http
  seq: 4
}

post {
  url: {{Server}}/api/v1/direcciones-coordinaciones-emergencias/coordinaciones-pma
  body: json
  auth: inherit
}

body:json {
  {
    "idRegistroActualizacion": 1,
    "idSuceso": {{IdSuceso}},
    "coordinaciones": []
  }
}

vars:post-response {
  IdDireccionCoordinacionEmergencia: res.body.idDireccionCoordinacionEmergencia
}

assert {
  res.status: eq 200
}
